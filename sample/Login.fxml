<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="mainframe" prefHeight="500.0" prefWidth="320.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.LoginController">
   <children>
      <Label layoutX="115.0" layoutY="235.0" text="Username:" textFill="#d70f0f">
         <font>
            <Font name="Luminari" size="18.0" />
         </font></Label>
      <TextField fx:id="username" layoutX="45.0" layoutY="288.0" promptText="Madhav">
         <font>
            <Font size="18.0" />
         </font></TextField>
      <Button fx:id="signin" layoutX="128.0" layoutY="432.0" mnemonicParsing="false" onAction="#make_account" text="Sign up" />
      <Button fx:id="signup" layoutX="130.0" layoutY="343.0" mnemonicParsing="false" onAction="#login" text="Sign in" />
   </children>
</AnchorPane>
